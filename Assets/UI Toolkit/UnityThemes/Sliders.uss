.Slider .unity-base-slider__dragger-border {
    background-image: url('project://database/Assets/Sprites/new/%D0%9D%D0%B0%D1%81%D1%82%D1%80%D0%BE%D0%B9%D0%BA%D0%B8/%D0%A1%D0%B2%D0%B8%D1%82%D1%87%D0%B5%D1%80%20%D0%B6%D0%B5%D0%BB%D1%82%D1%8B%D0%B9.png?fileID=21300000&guid=b57cde04e1c037a4496f4a3ffeba45d2&type=3#Свитчер желтый');
